android_app {
    srcs: ["java/**/*.java", "java/**/*.kt"],
    resource_dirs: ["res"],
    name: "Backup",
    certificate: "platform",
    privileged: true,
    optimize: {
        enabled: false,
    },
    required: [
        "permissions_com.stevesoltys.backup.xml",
        "whitelist_com.stevesoltys.backup.xml"
    ],
    static_libs: [
        "BIP39",
        "commons-io",
        "constraintlayout",
        "core-ktx",
        "lifecycle-extensions",
        "material",
        "preference-ktx"
    ]
}